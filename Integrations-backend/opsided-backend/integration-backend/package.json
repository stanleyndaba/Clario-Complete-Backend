{
  "name": "integrations-backend",
  "version": "1.0.0",
  "description": "Production-grade backend for Opside's Integrations Hub",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:oauth": "jest --testNamePattern='OAuth'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "db:reset": "npm run migrate:rollback && npm run migrate && npm run seed",
    "health": "curl http://localhost:3001/health",
    "swagger": "swagger-jsdoc -d swaggerDef.js -o swagger.json"
  },
  "keywords": ["integrations", "oauth", "amazon", "gmail", "stripe", "api", "backend"],
  "author": "Opside",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.2",
    "socket.io": "^4.7.4",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "compression": "^1.7.4",
    "express-http-proxy": "^1.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "firebase-admin": "^12.0.0",
    "aws-sdk": "^2.1499.0",
    "googleapis": "^128.0.0",
    "stripe": "^14.7.0",
    "amazon-sp-api": "^0.8.0",
    "node-cache": "^5.1.2",
    "express-slow-down": "^2.0.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-request-id": "^1.4.1",
    "express-status-monitor": "^1.3.4",
    "express-pino-logger": "^7.0.0",
    "pino": "^8.17.0",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.8",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "@types/sharp": "^0.32.0",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.1",
    "prettier": "^3.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opsided/integrations-backend.git"
  },
  "bugs": {
    "url": "https://github.com/opsided/integrations-backend/issues"
  },
  "homepage": "https://github.com/opsided/integrations-backend#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts",
      "!src/config/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
} 

# Skip Puppeteer Chrome download during npm install
# This prevents build timeouts on Render while still allowing
# Puppeteer to use system Chrome if available at runtime
puppeteer_skip_chromium_download=true

# Speed up npm install
fetch-retries=2
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000
fetch-timeout=300000

# Reduce verbosity
loglevel=warn
